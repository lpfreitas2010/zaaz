            {if $i2 == 'usuario'}
                {if $i.nome != ""} <strong>USU√ÅRIO:</strong> {$i.nome} <br>{/if}
            {/if}

			<!-- EMAIL DE -->
            {if $i2 == 'email_de'}
                {if $i.email_re != ""} <strong>DE:</strong> {$i.email_re} <br>{/if}
            {/if}

            <!-- EMAIL PARA -->
            {if $i2 == 'email_para'}
                {if $i.email != ""} <strong>PARA:</strong> {$i.email} <br>{/if}
            {/if}

            <!-- STATUS -->
            {if $i2 == 'status'}
                {if $i.status != ""} <strong>STATUS:</strong> {$i.status} <br>{/if}
            {/if}

            <!-- MENSAGEM -->
