        {include file="modulos/geral/pdf/includes/content_header.phtml"}

        <!-- OUTROS CAMPOS -->
        {foreach $campos as $i2}

            <!-- IP -->
            {if $i2 == 'ip'}
                {if $i.IP != ""} <strong>IP:</strong> {$i.IP} <br>{/if}
            {/if}

            <!-- SO -->
            {if $i2 == 'so'}
                {if $i.SO != ""} <strong>SO:</strong> {$i.SO} <br>{/if}
